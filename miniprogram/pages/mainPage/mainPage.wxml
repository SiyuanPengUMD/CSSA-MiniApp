<!--index.wxml-->
<view class="container" bindtap="onClicked">
  <!-- background img -->
  <view style="width:{{winWidth}}px;height:{{imgHeight}}px;" class="bgimg">
    <image src="/images/terp_shell_2.png" style="height:{{imgHeight}}px; width:{{imgWidth}}px; float:right"/>
  </view>

  <!--Navigation Bar-->
  <view class="navbars_container">
    <view class="navbars">
      <view class="navbar">
        <navigator url="mainPage" hover-class="navigator-hover">
          <view class="nav_img">
            <icon class="iconfont icon-shouye"></icon>
          </view>
          <view class="nav_text"> \n 主页</view>

        </navigator>
      </view>

      <view class="navbar">
        <navigator url="../users/users" hover-class="navigator-hover">
          <view class="nav_img">
            <icon class="iconfont icon-fenleiorguangchangorqita"></icon>
          </view>
          <view class="nav_text"> \n 活动</view>
        </navigator>
      </view>

      <view class="navbar">
        <navigator url="../loginPage/loginPage" hover-class="navigator-hover">
          <view class="nav_img">
            <icon class="iconfont icon-shezhi"></icon>
          </view>
          <view class="nav_text"> \n 设置</view>
        </navigator>

      </view>
    </view>
  </view>
</view>